<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.eyetracking">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera.any" android:required="true"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@drawable/container_calibration"
        android:supportsRtl="true"
        android:theme="@style/Theme.EyeTracking"
        tools:targetApi="31">

        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AgreementActivity" android:exported="true" />
        <activity android:name=".GenderActivity" android:exported="true" />
        <activity android:name=".InstructionActivity" android:exported="true" />
        <activity android:name=".CalibrationActivity" android:exported="true" />
        <activity android:name=".CalibrationdirectorActivity" android:exported="true" />
        <activity android:name=".CalibrationreddotActivity" android:exported="true" />
        <activity android:name=".VideoPlayActivity" android:exported="true" />
        <activity android:name=".VideoChooseActivity" android:exported="true" />
        <activity android:name=".DataOutputActivity" android:exported="true" />
        <activity android:name=".Exit" android:exported="true" />

    </application>
</manifest>
